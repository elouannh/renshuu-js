    "eslint": "^9.11.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.26.7",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.6.0"
