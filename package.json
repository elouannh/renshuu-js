    "email": "ehosta@student.42lyon.fr",
    "name": "Elouann H."
  },
  "contributors": [
    {
      "email": "ehosta@student.42lyon.fr",
      "name": "Elouann H."
    }
  ],
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/elouannh/dashboard"
    }
  ],
  "files": [
    "dist",
    "typing",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "main": "dist/index.js",
  "types": "typing/index.d.ts",
  "directories": {
    "lib": "dist"
  },
  "scripts": {
    "format": "prettier -w src",
