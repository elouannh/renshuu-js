{
  "name": "renshuu-js",
  "version": "0.0.0",
  "description": "Get your Renshuu statistics and manage your schedules with this powerful NPM package. Interact with the Renshuu API and integrate your data anywhere. ",
  "keywords": [
    "api",
    "npm",
    "package",
    "japanese",
    "renshuu"
  ],
  "homepage": "https://github.com/elouannh/renshuu-js#readme",
  "bugs": {
    "url": "https://github.com/elouannh/renshuu-js/issues"
  },
  "license": "MIT",
