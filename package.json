    "doc": "node scripts/docbuilder.js",
    "dev": "npm run build && node dist/tests/index.js"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "chalk": "^3.0.0-beta.2",
    "dotenv": "^16.4.5",
